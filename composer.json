{
  "name": "starchitect/comparison-samples",
  "type": "project",
  "description": "Samples for the Starchitect Comparison library",
  "keywords": [
    "comparison",
    "comparable",
    "comparator",
    "library",
    "sample"
  ],
  "homepage": "https://github.com/starchitect/comparison-samples",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Anton Vorobyev",
      "email": "anton@vorobyev.co",
      "homepage": "https://vorobyev.co",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "~5.6|~7.0",
    "starchitect/comparison": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 || ^6.0"
  },
  "autoload": {
    "psr-4": {
      "Starchitect\\ComparisonSamples\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Starchitect\\ComparisonSamples\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "repositories": [
    {
        "type": "path",
        "url": "../starchitect-comparison"        
    }
  ]
}
